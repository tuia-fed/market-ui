{
  "name": "market-ui",
  "version": "0.1.0",
  "description": "市场营销组件库",
  "main": "lib/mkui.umd.min.js",
  "types": "./types/index.d.ts",
  "keywords": [
    "vue",
    "ui",
    "vueComponents"
  ],
  "files": [
    "lib",
    "types"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "npm run docs:dev",
    "build": "npm run build:lib && npm run build:components && npm run docs:build",
    "build:lib": "vue-cli-service build --target lib --name mkui --dest lib src/index.js",
    "build:components": "rollup -c",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vuepress/plugin-active-header-links": "^1.8.2",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.44.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-vue": "5.1.9",
    "vue-highlightjs": "^1.3.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2"
  }
}
